{
  "name": "nestor",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.19.2",
    "better-docs": "^2.3.2",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "electron-dl": "^1.14.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "express": "^4.17.1",
    "fuzzball": "^1.3.0",
    "is-electron": "^2.2.0",
    "jquery": "^3.5.1",
    "jsdoc": "^3.6.5",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4",
    "papaparse": "^5.3.0",
    "pouchdb": "^7.2.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-chartjs-2": "^2.10.0",
    "react-csv": "^1.1.2",
    "react-dom": "^16.13.1",
    "react-file-reader": "^1.1.4",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.0.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "wait-on": "^3.3.0",
    "zerorpc": "^0.9.8"
  },
  "scripts": {
    "init": "npm install && ./node_modules/.bin/electron-rebuild",
    "init-windows": "npm install && ./node_modules/.bin/electron-rebuild.cmd",
    "start": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "electron-builder",
    "build-python": "pyinstaller api.spec --additional-hooks-dir=. --distpath pythondist --noconfirm",
    "build": "npm run build-react && npm run build-python && npm run dist",
    "docs": "jsdoc -c jsdoc.conf.json",
    "docs2md": "generatedocs.sh "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "electron": "^18.3.7",
    "electron-builder": "^22.8.0",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.11.0",
    "jsdoc-to-markdown": "^7.0.0",
    "typescript": "^3.9.7"
  },
  "build": {
    "appId": "com.nist.nestor",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": "pythondist/**/*",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    },
    "npmRebuild": false
  }
}